{
	"name": "@mckayla/electron-redux",
	"version": "3.0.2",
	"author": "McKayla Washburn <mckayla@hey.com>",
	"description": "Make sure all your stores are on the same page",
	"keywords": [
		"electron",
		"redux"
	],
	"license": "MIT",
	"homepage": "https://github.com/aslilac/electron-redux",
	"repository": "https://github.com/aslilac/electron-redux",
	"bugs": "https://github.com/aslilac/electron-redux/issues",
	"type": "commonjs",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"engines": {
		"node": ">= 14"
	},
	"files": [
		"build/**",
		"preload.d.ts",
		"preload.js",
		"renderer.d.ts",
		"renderer.js"
	],
	"peerDependencies": {
		"electron": ">=11.0.0",
		"redux": ">=4.0.0"
	},
	"devDependencies": {
		"@mckayla/tsconfig": "^1.0.3",
		"@types/jest": "^29.2.4",
		"esbuild": "^0.16.10",
		"jest": "^29.3.1",
		"ts-jest": "^29.0.3",
		"typescript": "^4.9.4"
	},
	"scripts": {
		"build": "tsc -b . && esbuild --bundle --external:electron --outfile=preload.js ./preload.in.js && esbuild --bundle --external:electron --outfile=renderer.js ./renderer.in.js",
		"lint": "eslint .",
		"prepack": "$npm_execpath run build -s",
		"test": "jest"
	}
}
